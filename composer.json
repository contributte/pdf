{
  "name": "contributte/pdf",
  "description": "Pdf response extension for Nette Framework",
  "keywords": [
    "nette",
    "contributte",
    "php",
    "mpdf",
    "pdf"
  ],
  "type": "library",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/contributte/pdf",
  "authors": [
    {
      "name": "Miroslav Paulík",
      "email": "miras.paulik@seznam.cz"
    },
    {
      "name": "Jan Kuchař"
    },
    {
      "name": "Tomáš Votruba",
      "email": "tomas.vot@gmail.com"
    },
    {
      "name": "Petr Parolek",
      "homepage": "https://www.webnazakazku.cz/"
    },
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "mpdf/mpdf": "^8.1.0",
    "nette/application": "^3.1.0",
    "nette/http": "^3.2.0"
  },
  "require-dev": {
    "nette/di": "^3.1.2",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "symfony/css-selector": "^6.3.0",
    "symfony/dom-crawler": "^6.3.0"
  },
  "conflict": {
    "masterminds/html5": "<2.8.0"
  },
  "suggest": {
    "symfony/dom-crawler": "Allows filtering html tags.",
    "nette/nette": "PHP framework to which this extension belongs to."
  },
  "autoload": {
    "psr-4": {
      "Contributte\\PdfResponse\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "7.1.x-dev"
    }
  }
}
