{
  "name": "contributte/pdf",
  "description": "Pdf response extension for Nette Framework",
  "keywords": [
    "nette",
    "contributte",
    "php",
    "mpdf",
    "pdf"
  ],
  "type": "library",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/contributte/pdf",
  "authors": [
    {
      "name": "Miroslav Paulík",
      "email": "miras.paulik@seznam.cz"
    },
    {
      "name": "Jan Kuchař"
    },
    {
      "name": "Tomáš Votruba",
      "email": "tomas.vot@gmail.com"
    },
    {
      "name": "Petr Parolek",
      "homepage": "https://www.webnazakazku.cz/"
    },
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.1",
    "mpdf/mpdf": "^8.0",
    "nette/application": "~3.0",
    "nette/http": "~3.0"
  },
  "require-dev": {
    "latte/latte": "^2.10",
    "nette/di": "^3.0.0",
    "ninjify/nunjuck": "^0.3.0",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "symfony/css-selector": "^4.4|^5.0",
    "symfony/dom-crawler": "^4.4|^5.0"
  },
  "suggest": {
    "symfony/dom-crawler": "Allows filtering html tags.",
    "nette/nette": "PHP framework to which this extension belongs to."
  },
  "autoload": {
    "psr-4": {
      "Contributte\\PdfResponse\\": "src/"
    },
    "files": [
      "src/compatibility.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.1-dev"
    }
  }
}
